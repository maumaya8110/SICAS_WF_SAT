          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="Reporte_CobranzaAtencionYControlTableAdapter" GeneratorDataComponentClassName="Reporte_CobranzaAtencionYControlTableAdapter" Name="Reporte_CobranzaAtencionYControl" UserDataComponentName="Reporte_CobranzaAtencionYControlTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="SICASCentralConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT	C.Contrato_ID,	C.Conductor_ID,
		U.NumeroEconomico Unidad,
		(COND.Apellidos + ' ' + COND.Nombre) Conductor,
		--	Rentas
		(SELECT ISNULL(SUM(Abono - Cargo),0)
		FROM	CuentaConductores
		WHERE	Conductor_ID = COND.Conductor_ID
		AND		Cuenta_ID = 1)	Rentas,
		--	Otros Cargos
		(SELECT ISNULL(SUM(Abono - Cargo),0)
		FROM	CuentaConductores
		WHERE	Conductor_ID = COND.Conductor_ID
		AND		Cuenta_ID = 2)	OtrosCargos,
		--	Cargos Estacion
		(SELECT ISNULL(SUM(Abono - Cargo),0)
		FROM	CuentaConductores
		WHERE	Conductor_ID = COND.Conductor_ID
		AND		Cuenta_ID = 10)	CargosEstacion,
		--	SCP
		(SELECT ISNULL(SUM(Abono - Cargo),0)
		FROM	CuentaConductores
		WHERE	Conductor_ID = COND.Conductor_ID
		AND		Cuenta_ID = 8)	SCP,
		U.Kilometraje,
		COND.SaldoATratar,
		COND.Cronocasco,
		COND.TerminalDatos,
		COND.BloquearConductor,
		COND.MensajeACaja,
		(COP.Apellidos + ' ' + COP.Nombre) Copia,
		EC.Nombre Estatus
FROM	Contratos C
INNER JOIN	Conductores COND
ON		C.Conductor_ID = COND.Conductor_ID
INNER JOIN	Unidades U
ON		C.Unidad_ID = U.Unidad_ID
INNER JOIN	Conductores COP
ON		C.ConductorCopia_ID = COP.Conductor_ID
INNER JOIN	EstatusConductores	EC
ON		COND.EstatusConductor_ID = EC.EstatusConductor_ID
WHERE	C.Conductor_ID IN
(
	SELECT	Conductor_ID
	FROM	CuentaConductores
	GROUP BY	Conductor_ID
	HAVING	SUM(Abono - Cargo) &lt; 0
)
AND		(@NumeroEconomico IS NULL OR U.NumeroEconomico = @NumeroEconomico)
AND		(@Nombre IS NULL OR (COND.Apellidos + ' ' + COND.Nombre) LIKE @Nombre + '%')
</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="true" AutogeneratedName="NumeroEconomico" ColumnName="NumeroEconomico" DataSourceName="SICASCentral.dbo.Unidades" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@NumeroEconomico" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="Unidad" SourceColumnNullMapping="false" SourceVersion="Current" />
