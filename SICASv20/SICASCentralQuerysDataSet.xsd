                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>
SELECT	CE.Empresa_ID, E.Nombre Empresa,
		CE.Cuenta_ID, C.Nombre Cuenta,
		CE.Saldo,
		EC.Banco, EC.CuentaBancaria, EC.Referencia, EC.Comentarios
FROM
(
	SELECT	Empresa_ID, Cuenta_ID,
			(SUM(Abono) - SUM(Cargo)) Saldo
	FROM	CuentaEmpresas CE
	GROUP BY	Empresa_ID, Cuenta_ID
)	CE
INNER JOIN	Empresas E
ON	E.Empresa_ID = CE.Empresa_ID
INNER JOIN	Cuentas C
ON	C.Cuenta_ID = CE.Cuenta_ID
INNER JOIN	Empresas_Cuentas EC
ON	(CE.Empresa_ID = EC.Empresa_ID AND CE.Cuenta_ID = EC.Cuenta_ID)

</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
