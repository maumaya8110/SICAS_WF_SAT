            <MainSource>
              <DbSource ConnectionRef="SICASCentralConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT	Est.Nombre Estacion,
		U.NumeroEconomico,
		ISNULL(Cond.Nombre + ' ' + Cond.Apellidos,'') Conductor,
		ISNULL(LU.Nombre,'') Locacion,
		ISNULL(MU.Descripcion + ' ' + CONVERT(varchar(30),MU.Anio),'') Modelo,
		U.Placas, U.NumeroSerie,
		C.MontoDiario RentaDiaria,
		ISNULL(C.Descripcion + ' ' + CONVERT(varchar(30),C.MontoDiario),'') PlanDeCobro
FROM	Unidades	U
INNER JOIN	Estaciones Est
ON		U.Estacion_ID = Est.Estacion_ID
LEFT JOIN	Contratos	C
ON		C.Unidad_ID = U.Unidad_ID
LEFT JOIN	Conductores	Cond
ON		C.Conductor_ID = Cond.Conductor_ID
LEFT JOIN	LocacionesUnidades	LU
ON		LU.LocacionUnidad_ID = U.LocacionUnidad_ID
LEFT JOIN	ModelosUnidades	MU
ON		MU.ModeloUnidad_ID = U.ModeloUnidad_ID
WHERE	(@Estacion_ID IS NULL OR U.Estacion_ID = @Estacion_ID)</CommandText>
