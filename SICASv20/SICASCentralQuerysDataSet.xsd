                      <Parameter AllowDbNull="true" AutogeneratedName="Estacion_ID" ColumnName="Estacion_ID" DataSourceName="SICASCentral.dbo.Unidades" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@Estacion_ID" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="Estacion_ID" SourceColumnNullMapping="false" SourceVersion="Current" />
