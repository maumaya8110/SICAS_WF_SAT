<?xml version="1.0" standalone="yes"?>
<xs:schema id="ConfigDS" targetNamespace="http://tempuri.org/ConfigDS.xsd" xmlns:mstns="http://tempuri.org/ConfigDS.xsd" xmlns="http://tempuri.org/ConfigDS.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="ConfigDS" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="LocalConfig">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="AppId" type="xs:string" minOccurs="0" />
              <xs:element name="AppName" type="xs:string" minOccurs="0" />
              <xs:element name="MainFile" type="xs:string" minOccurs="0" />
              <xs:element name="CurrentVersion" type="xs:string" minOccurs="0" />
              <xs:element name="Description" type="xs:string" minOccurs="0" />
              <xs:element name="ImagePath" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="FTPSettings">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Order" type="xs:int" minOccurs="0" />
              <xs:element name="Server" type="xs:string" minOccurs="0" />
              <xs:element name="User" type="xs:string" minOccurs="0" />
              <xs:element name="Pwd" type="xs:string" minOccurs="0" />
              <xs:element name="Path" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="WebServiceSettings">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Order" type="xs:int" minOccurs="0" />
              <xs:element name="Url" type="xs:string" minOccurs="0" />
              <xs:element name="User" type="xs:string" minOccurs="0" />
              <xs:element name="Pwd" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="EmailSettings">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Order" type="xs:int" minOccurs="0" />
              <xs:element name="Server" type="xs:string" minOccurs="0" />
              <xs:element name="User" type="xs:string" minOccurs="0" />
              <xs:element name="Pwd" type="xs:string" minOccurs="0" />
              <xs:element name="Port" type="xs:int" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>